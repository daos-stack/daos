Part file:
path                     : $(nW)file.pool
type                     : regular file
size                     : $(nW)

POOL Header:
Signature                : PMEMOBJ
Major                    : $(*)
Mandatory features       : $(*)
Not mandatory features   : $(*)
Forced RO                : $(*)
Pool set UUID            : $(*)
UUID                     : $(*)
Previous part UUID       : $(*)
Next part UUID           : $(*)
Previous replica UUID    : $(*)
Next replica UUID        : $(*)
Creation Time            : $(*)
Alignment Descriptor     : $(*)
Class                    : $(*)
Data                     : $(*)
$(OPT)Machine                  : AMD X86-64
$(OPT)Machine                  : Aarch64
$(OPT)Machine                  : RISCV
$(OPX)Machine                  : PPC64
$(OPT)Last shutdown            : dirty
$(OPX)Last shutdown            : clean
Checksum                 : $(*) [OK]

PMEM OBJ Header:
$(OPT)Layout                   : pmempool$(*)
$(OPX)Layout                   : (null)
Lanes offset             : $(*)
Number of lanes          : $(*)
Heap offset              : $(*)
Heap size                : $(*)
Checksum                 : $(*) [OK]
Root offset              : $(*)
