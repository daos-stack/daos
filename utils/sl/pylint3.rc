[MESSAGES CONTROL]
disable=locally-disabled,locally-enabled,star-args,wrong-import-order,
        unused-wildcard-import,invalid-name,global-statement,bad-option-value,
        wrong-import-position,consider-using-f-string,unspecified-encoding

[FORMAT]
max-line-length=100
max-locals=30
max-args=10
min-similarity-lines=10
max-branches=25
max-statements=200
max-attributes=30

[MISCELLANEOUS]
# List of note tags to take in consideration, separated by a comma.
notes=FIXME,XXX,TODO

[REPORTS]
reports=no
