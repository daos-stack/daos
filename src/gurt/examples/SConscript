"""Example code"""


def scons():
    """Execute build"""
    Import('env')

    libs = ['gurt']
    if GetOption("test_coverage"):
        libs += ['gcov']
    env.d_test_program('telemetry_producer_example', 'telem_producer_example.c', LIBS=libs)
    env.d_test_program('telemetry_consumer_example', 'telem_consumer_example.c', LIBS=libs)


if __name__ == "SCons.Script":
    scons()
